require_relative "pregunta"

class  verdaderoofalso < Pregunta

	attr_accessor :vf

	def initialize(enunciado)

		super(enunciado)

		@verdaderoofalso = ["a) cierto","b) falso"]
	end

	def to_s

		cadena ="#{@enunciado}\n"

		@verdaderoofalso.each {|m| cadena += "#{m}\n"}
	
		cadena
        end

	
end